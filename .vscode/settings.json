{
  "eslint.format.enable": true,
  "eslint.codeAction.showDocumentation": {
    "indent": ["true", "spaces", 4],
    "skipStrings": true,
    "skipBlankLines": true,
    "enable": true
  },
  "eslint.trace.server": null,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript"
  ],
  "editor.formatOnSave": true
}